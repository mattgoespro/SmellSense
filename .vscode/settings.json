{
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "dart.analysisExcludedFolders": [
    ".config/",
    ".dart_tool/",
    ".git/",
    ".idea/",
    ".vscode/",
    "android/",
    "assets/",
    "build/",
    "coverage/",
    "ios/",
    "scripts/",
    "tools/"
  ],
  "dart.lineLength": 80,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "files.exclude": {
    // "**/.classpath": true,
    // "**/.factorypath": true,
    // "**/.project": true,
    // "**/.settings": true,
    // ".dart_tool": true,
    // ".flutter-plugins": true,
    // ".flutter-plugins-dependencies": true,
    // ".idea": true,
    // ".packages": true,
    // "pubspec.lock": true
  },
  "gradle.autoDetect": "on",
  "gradle.focusTaskInExplorer": true,
  "gradle.nestedProjects": true,
  "java.compile.nullAnalysis.mode": "automatic",
  "java.configuration.updateBuildConfiguration": "interactive",
  "java.import.gradle.java.home": "C:\\Users\\Matt\\AppData\\Local\\Programs\\Android Studio 2\\jbr",
  "json.schemas": [
    {
      "description": "Codemagic CI/CD file configuration",
      "fileMatch": ["codemagic.yml", "codemagic.yaml"],
      "url": "https://codemagic.io/codemagic-schema.json"
    }
  ],
  "terminal.integrated.defaultProfile.windows": "Git Bash"
}
