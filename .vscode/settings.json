{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[properties]": {
    "files.eol": "\n"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "dart.analysisExcludedFolders": [
    ".dart_tool/",
    ".git",
    ".husky/",
    ".idea/",
    "build/",
    "android/",
    "ios/",
    "tools/",
    "scripts/"
  ],
  "dart.analysisServerFolding": false,
  "dart.cliConsole": "debugConsole",
  "dart.debugSdkLibraries": false,
  "dart.flutterOutline": true,
  "dart.flutterTestLogFile": "test_results.xml",
  "dart.runPubGetOnPubspecChanges": "prompt",
  "dart.showTestCodeLens": true,
  "dart.vmServiceLogFile": "dart-tools.log",
  "editor.codeActionsOnSave": {
    "source.fixAll.dart": "explicit",
    "source.organizeImports": "always",
    "source.organizeLinkDefinitions": "always"
  },
  "emeraldwalk.runonsave": {
    "commands": []
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".idea": true,
    ".packages": true,
    "pubspec.lock": true
  },
  "json.schemas": [
    {
      "description": "Codemagic CI/CD file configuration",
      "fileMatch": ["codemagic.yml", "codemagic.yaml"],
      "url": "https://codemagic.io/codemagic-schema.json"
    }
  ],
  "terminal.integrated.defaultProfile.windows": "Git Bash"
}
