{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "shakram02.bash-beautify",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[properties]": {
    "files.eol": "\n"
  },
  "[gradle]": {
    "files.eol": "\n",
    "editor.defaultFormatter": "vscjava.vscode-gradle",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[gradle-build]": {
    "files.eol": "\n",
    "editor.defaultFormatter": "vscjava.vscode-gradle",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  // "[xml]": {
  //   "editor.defaultFormatter": "redhat.vscode-xml"
  // },
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "dart.renameFilesWithClasses": "prompt",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "always"
  },
  "gradle.nestedProjects": true,
  "java.gradle.buildServer.openBuildOutput": "openOnBuildStart",
  "gradle.allowParallelRun": true,
  "gradle.autoDetect": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".idea": true,
    ".packages": true,
    "pubspec.lock": true
  },
  "terminal.integrated.profiles.windows": {
    "Git Bash": {
      "path": "C:\\Program Files\\Git\\bin\\bash.exe",
      "args": ["-i"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "yaml.schemas": {
    "https://static.codemagic.io/codemagic-schema.json": "https://codemagic.io/codemagic-schema.json"
  },
  "gradle.debug": true
}
