{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug: wi-fi device",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "SM A245F",
      "flutterMode": "debug",
      "console": "debugConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "debug",
        "order": 1
      }
    },
    {
      "name": "debug: virtual device",
      "type": "dart",
      "request": "attach",
      "program": "lib/main.dart",
      "deviceId": "emulator-5554",
      "presentation": {
        "group": "debug",
        "order": 1
      },
      "preLaunchTask": "adb: launch virtual device"
    },
    {
      "name": "debug: launch virtual device",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "presentation": {
        "group": "debug",
        "order": 2
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "test: integration",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/test",
      "args": ["--coverage", "--reporter=expanded"]
    }
  ]
}
