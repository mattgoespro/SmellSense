{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug: device over wi-fi",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "SM A245F",
      "console": "debugConsole",
      "flutterMode": "debug",
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "debug",
        "order": 1
      },
      "preLaunchTask": "adb: connect to wifi device"
    },
    {
      "name": "dart: launch virtual device",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "debug",
        "order": 1
      }
    },
    {
      "name": "test: integration",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/test",
      "args": ["--coverage", "--reporter=expanded"]
    }
  ]
}
