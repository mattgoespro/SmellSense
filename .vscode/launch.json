{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug: virtual device",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "debug",
        "order": 1
      },
      "console": "debugConsole",
      "codeLens": {
        "for": ["debug-file", "run-file"],
        "title": "Debug",
        "path": "${relativeFile}"
      },
      "flutterMode": "debug",
      "vmAdditionalArgs": [
        "--enable-software-rendering",
        "--enable-dart-profiling",
        "--disable-observatory",
        "--disable-service-auth-codes",
        "--no-sound-null-safety"
      ]
    },
    {
      "name": "debug: device over wi-fi",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "SM A245F",
      "console": "debugConsole",
      "flutterMode": "debug",
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "debug",
        "order": 2
      },
      "preLaunchTask": "adb: connect to wifi device"
    },
    {
      "name": "test: integration",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/test",
      "args": ["--coverage", "--reporter=expanded"]
    }
  ]
}
